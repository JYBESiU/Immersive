<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.thekaist">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/genius"
        android:label="@string/app_name"
        android:usesCleartextTraffic="true"
        android:roundIcon="@drawable/genius"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.thekaist.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.thekaist.FrontActivity"/>
        <activity android:name="com.example.thekaist.GameActivity" />
        <activity android:name="com.example.thekaist.changeUserinfo"></activity>
    </application>

</manifest>